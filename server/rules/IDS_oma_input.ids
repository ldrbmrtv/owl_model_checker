<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ids:ids xmlns:ids="http://standards.buildingsmart.org/IDS" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/1.0/ids.xsd">
    <ids:info>
        <ids:title>OMA - Occupant Movement Analysis</ids:title>
        <ids:copyright>buildingSMART International</ids:copyright>
        <ids:version>2.2</ids:version>
        <ids:description>Minimum requirements for IFC models to be used in Occupant Movement
      Analysis</ids:description>
    </ids:info>
    <ids:specifications>
        <ids:specification description="Occupant Movement Analysis - data properties list for Building" ifcVersion="IFC4X3_ADD2" name="Building">
            <ids:applicability minOccurs="1" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <ids:simpleValue>IFCBUILDING</ids:simpleValue>
                    </ids:name>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:property cardinality="optional" dataType="IFCLABEL" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/OccupancyType">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_BuildingCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>OccupancyType</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Assembly"/>
                            <xs:enumeration value="Ambulatory Health Care"/>
                            <xs:enumeration value="Educational"/>
                            <xs:enumeration value="Day Care"/>
                            <xs:enumeration value="Health Care"/>
                            <xs:enumeration value="Detention and Correctional"/>
                            <xs:enumeration value="One- and Two-Family Dwelling"/>
                            <xs:enumeration value="Lodging or Rooming House"/>
                            <xs:enumeration value="Hotels and Dormitory"/>
                            <xs:enumeration value="Apartment"/>
                            <xs:enumeration value="Mercantile"/>
                            <xs:enumeration value="Business"/>
                            <xs:enumeration value="Industrial"/>
                            <xs:enumeration value="Storage"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for Storey" ifcVersion="IFC4X3_ADD2" name="Storey">
            <ids:applicability minOccurs="1" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <ids:simpleValue>IFCBUILDINGSTOREY</ids:simpleValue>
                    </ids:name>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute instructions="Storey name/number">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[\S\s]+[\S]+"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:property dataType="IFCLENGTHMEASURE" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/ElevationOfFFLRelative">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_BuildingStoreyCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>ElevationOfFFLRelative</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for Space" ifcVersion="IFC4X3_ADD2" name="Space">
            <ids:applicability minOccurs="1" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <ids:simpleValue>IFCSPACE</ids:simpleValue>
                    </ids:name>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute cardinality="optional" instructions="Descriptive name">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[\S\s]+[\S]+"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:property cardinality="optional" dataType="IFCCOUNTMEASURE" instructions="Maximal number of people required for the activity assigned to this space in peak time" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/OccupancyNumberPeak">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_SpaceOccupancyRequirements</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>OccupancyNumberPeak</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCAREAMEASURE" instructions="Design occupancy loading for this type of usage assigned to this space" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/OccupancyNumberPeak">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_SpaceOccupancyRequirements</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>AreaPerOccupant</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCBOOLEAN" instructions="Indicates if the space contains a fire exit" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/FireExit">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_SpaceFireSafetyRequirements</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>FireExit</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCBOOLEAN" instructions="Indication whether this space (in case of e.g., a toilet) is designed to serve as a publicly accessible space, e.g., for a public toilet (TRUE) or not (FALSE)" uri="https://identifier.buildingsmart.org/uri/buildingsmart/ifc/4.3/prop/HandicapAccessible">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_SpaceCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>HandicapAccessible</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCREAL" instructions="The maximum flow rate, representing the  designed, theoretical or expected rate for occupants to move through a space under normal conditions">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_SpaceOccupancyRequirements</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>DesignPeopleFlowRate</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCLENGTHMEASURE" instructions="The clear width, the distance between walls or other obtacles, for example width of the corridor">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_SpaceAnalyticalQuantities</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>NominalWidth</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>					
                </ids:property>
				<ids:property cardinality="optional" dataType="IFCLENGTHMEASURE" instructions="The clear length representing the  travel distance  through the space, for example the travel distance along a corridor">
					<ids:propertySet>
                        <ids:simpleValue>ePset_OMA_SpaceAnalyticalQuantities</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>NominalLength</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
				</ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for Spatial Zone" ifcVersion="IFC4X3_ADD2" name="Spatial Zone">
            <ids:applicability minOccurs="0" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <ids:simpleValue>IFCSPATIALZONE</ids:simpleValue>
                    </ids:name>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute instructions="Unique name or title assigned to the spatial zone">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
                    </ids:name>
                </ids:attribute>
                <ids:attribute instructions="General information about the spatial zone">
                    <ids:name>
                        <ids:simpleValue>Description</ids:simpleValue>
                    </ids:name>
                </ids:attribute>
                <ids:attribute instructions="The type or function of the spatial zone">
                    <ids:name>
                        <ids:simpleValue>PredefinedType</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="CONSTRUCTION"/>
                            <xs:enumeration value="FIRESAFETY"/>
                            <xs:enumeration value="LIGHTING"/>
                            <xs:enumeration value="OCCUPANCY"/>
                            <xs:enumeration value="SECURITY"/>
                            <xs:enumeration value="THERMAL"/>
                            <xs:enumeration value="TRANSPORT"/>
							<xs:enumeration value="VENTILATION"/>
                            <xs:enumeration value="USERDEFINED"/>
                            <xs:enumeration value="NOTDEFINED"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:property cardinality="optional" dataType="IFCLABEL" instructions="The general type of occupant movement intended to be used for the spatial zone">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_SpatialZone</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>MovementType</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="COUNTER"/>
                            <xs:enumeration value="DESTINATION"/>
                            <xs:enumeration value="ENTRY"/>
                            <xs:enumeration value="EXIT"/>
                            <xs:enumeration value="GRANDSTAND"/>
                            <xs:enumeration value="HAZARD"/>
                            <xs:enumeration value="IMPASSABLE"/>
                            <xs:enumeration value="ORIGIN"/>
                            <xs:enumeration value="PASSAGEWAY"/>
                            <xs:enumeration value="REFUGE"/>
                            <xs:enumeration value="RESPONSE_TIME"/>
                            <xs:enumeration value="SIGNAGE"/>
                            <xs:enumeration value="TERRACING"/>
                            <xs:enumeration value="TIERED_SEATING"/>
                            <xs:enumeration value="WAITING"/>
                            <xs:enumeration value="WAYPOINT"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for Door" ifcVersion="IFC4X3_ADD2" name="Door">
            <ids:applicability minOccurs="1" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <xs:restriction base="xs:string">
                             <xs:enumeration value="IFCDOOR"/> 
                             <!-- <xs:enumeration value="IFCDOORTYPE"/> -->
                        </xs:restriction>
                    </ids:name>
                    <ids:predefinedType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DOOR"/>
                            <xs:enumeration value="TRAPDOOR"/>
                            <xs:enumeration value="USERDEFINED"/>
                            <xs:enumeration value="NOTDEFINED"/>
                        </xs:restriction>
                    </ids:predefinedType>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute instructions="Descriptive name">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[\S\s]+[\S]+"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:attribute instructions="Number of leaves - Also impacts direction of opening, i.e. RHR, LHR etc, see Direction below">
                    <ids:name>
                        <ids:simpleValue>OperationType</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DOUBLE_DOOR_DOUBLE_SWING"/>
                            <xs:enumeration value="DOUBLE_DOOR_FOLDING"/>
                            <xs:enumeration value="DOUBLE_DOOR_LIFTING_VERTICAL"/>
                            <xs:enumeration value="DOUBLE_DOOR_SINGLE_SWING"/>
                            <xs:enumeration value="DOUBLE_DOOR_SINGLE_SWING_OPPOSITE_LEFT"/>
                            <xs:enumeration value="DOUBLE_DOOR_SINGLE_SWING_OPPOSITE_RIGHT"/>
                            <xs:enumeration value="DOUBLE_DOOR_SLIDING"/>
                            <xs:enumeration value="DOUBLE_SWING_LEFT"/>
                            <xs:enumeration value="DOUBLE_SWING_RIGHT"/>
                            <xs:enumeration value="FOLDING_TO_LEFT"/>
                            <xs:enumeration value="FOLDING_TO_RIGHT"/>
                            <xs:enumeration value="LIFTING_HORIZONTAL"/>
                            <xs:enumeration value="LIFTING_VERTICAL_LEFT"/>
                            <xs:enumeration value="LIFTING_VERTICAL_RIGHT"/>
                            <xs:enumeration value="REVOLVING"/>
                            <xs:enumeration value="REVOLVING_VERTICAL"/>
                            <xs:enumeration value="ROLLINGUP"/>
                            <xs:enumeration value="SINGLE_SWING_LEFT"/>
                            <xs:enumeration value="SINGLE_SWING_RIGHT"/>
                            <xs:enumeration value="SLIDING_TO_LEFT"/>
                            <xs:enumeration value="SLIDING_TO_RIGHT"/>
                            <xs:enumeration value="SWING_FIXED_LEFT"/>
                            <xs:enumeration value="SWING_FIXED_RIGHT"/>
                            <xs:enumeration value="USERDEFINED"/>
                            <xs:enumeration value="NOTDEFINED"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:property cardinality="optional" dataType="IFCLENGTHMEASURE" instructions="The effective clear width  is the width of the opening measured at right angles to the wall in which the door is situated from the outside of the door stop on the closing side, to any obstruction on the hinge side">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_Door</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>EffectiveWidth</ids:simpleValue>
                    </ids:baseName>
                </ids:property>               
                <ids:property cardinality="optional" dataType="IFCBOOLEAN" instructions="Indicates if door will automatically close in the event of an alarm being set">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_DoorCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>SelfClosing</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property dataType="IFCBOOLEAN" instructions="Indicates if door will automatically close in the event of an alarm being set">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_DoorCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>IsExternal</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property dataType="IFCBOOLEAN" instructions="Indicates whether the door is a fire exit">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_DoorCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>FireExit</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCBOOLEAN" instructions="Flag to indicate of an accessible door">
                    <ids:propertySet>
                        <ids:simpleValue>Pset_DoorCommon</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>HandicapAccessible</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCREAL" instructions="The maximum flow rate, representing the  designed, theoretical or expected rate for occupants to move through a door  under normal conditions">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_Door</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>DesignPeopleFlowRate</ids:simpleValue>
                    </ids:baseName>
                </ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for Stairs" ifcVersion="IFC4X3_ADD2" name="Stairs">
            <ids:applicability minOccurs="0" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="IFCSTAIR"/>
                            <!-- <xs:enumeration value="IFCSTAIRTYPE"/> -->
                            <xs:enumeration value="IFCSTAIRFLIGHT"/>
                            <!-- <xs:enumeration value="IFCSTAIRFLIGHTTYPE"/> -->
                        </xs:restriction>
                    </ids:name>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute instructions="Descriptive name">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:pattern value="[\S\s]+[\S]+"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
				<ids:attribute cardinality="optional" instructions="Predefined generic type for a stair that is specified in an enumeration">
                    <ids:name>
                        <ids:simpleValue>PredefinedType</ids:simpleValue>
                    </ids:name>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="CURVED_RUN_STAIR"/>
                            <xs:enumeration value="DOUBLE_RETURN_STAIR"/>
                            <xs:enumeration value="HALF_TURN_STAIR"/>
                            <xs:enumeration value="HALF_WINDING_STAIR"/>
                            <xs:enumeration value="LADDER"/>
                            <xs:enumeration value="QUARTER_TURN_STAIR"/>
                            <xs:enumeration value="QUARTER_WINDING_STAIR"/>
                            <xs:enumeration value="SPIRAL_STAIR"/>
                            <xs:enumeration value="STRAIGHT_RUN_STAIR"/>
							<xs:enumeration value="THREE_QUARTER_TURN_STAIR"/>
							<xs:enumeration value="THREE_QUARTER_WINDING_STAIR"/>
							<xs:enumeration value="TWO_CURVED_RUN_STAIR"/>
							<xs:enumeration value="TWO_QUARTER_TURN_STAIR"/>
							<xs:enumeration value="TWO_QUARTER_WINDING_STAIR"/>
							<xs:enumeration value="TWO_STRAIGHT_RUN_STAIR"/>
							<xs:enumeration value="USERDEFINED"/>
							<xs:enumeration value="NOTDEFINED"/>
                        </xs:restriction>
                    </ids:value>
                </ids:attribute>
                <ids:property cardinality="optional" dataType="IFCPOSITIVELENGTHMEASURE" instructions="Vertical height of each step">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Pset_StairCommon"/>
                            <xs:enumeration value="Pset_StairFlightCommon"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>RiserHeight</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property dataType="IFCCOUNTMEASURE" instructions="Number of treads is a function of the number of risers">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Pset_StairCommon"/>
                            <xs:enumeration value="Pset_StairFlightCommon"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>NumberOfRiser</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:integer">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCPOSITIVELENGTHMEASURE" instructions="Tread depth includes nosing overhang">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Pset_StairCommon"/>
                            <xs:enumeration value="Pset_StairFlightCommon"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>TreadLength</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCLENGTHMEASURE" instructions="Tread depth includes nosing overhang">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Pset_StairCommon"/>
                            <xs:enumeration value="Pset_StairFlightCommon"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>NosingLength</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0.00"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property dataType="IFCCOUNTMEASURE" instructions="Total number of treads in the flight">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Pset_StairCommon"/>
                            <xs:enumeration value="Pset_StairFlightCommon"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>NumberOfTreads</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:integer">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCLENGTHMEASURE" instructions="The effective width represents the clear, usable width that allows people to move up or down the stairs unhindered">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ePset_OMA_Stair"/>
                            <xs:enumeration value="ePset_OMA_StairFlight"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>EffectiveWidth</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCCOUNTMEASURE" instructions="The maximum number of people that can safely use the stairs at any given time">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_Stair</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>OccupancyNumberPeak</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:integer">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCREAL" instructions="The maximum flow rate represents the designed, theoretical, or expected rate of people travelling on the stairs under normal conditions">
                    <ids:propertySet>
                        <ids:simpleValue>ePset_OMA_Stair</ids:simpleValue>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>DesignPeopleFlowRate</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:double">
                            <xs:minExclusive value="0"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
                <ids:property cardinality="optional" dataType="IFCLABEL" instructions="The expected direction of travel on stairs">
                    <ids:propertySet>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="ePset_OMA_Stair"/>
                            <xs:enumeration value="ePset_OMA_StairFlight"/>
                        </xs:restriction>
                    </ids:propertySet>
                    <ids:baseName>
                        <ids:simpleValue>TravelDirection</ids:simpleValue>
                    </ids:baseName>
                    <ids:value>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="DOWNWARDS"/>
                            <xs:enumeration value="UPWARDS"/>
							<xs:enumeration value="BIDIRECTIONAL"/>
                        </xs:restriction>
                    </ids:value>
                </ids:property>
            </ids:requirements>
        </ids:specification>
        <ids:specification description="Occupant Movement Analysis - data properties list for transport element (Lift/Elevator)" ifcVersion="IFC4X3_ADD2" name="Transport element (Lift/Elevator)">
            <ids:applicability minOccurs="0" maxOccurs="unbounded">
                <ids:entity>
                    <ids:name>
                        <ids:simpleValue>IFCTRANSPORTELEMENT</ids:simpleValue>
                    </ids:name>
                    <ids:predefinedType>
                        <ids:simpleValue>ELEVATOR</ids:simpleValue>
                    </ids:predefinedType>
                </ids:entity>
            </ids:applicability>
            <ids:requirements>
                <ids:attribute instructions="Identifier used to identify the lift">
                    <ids:name>
                        <ids:simpleValue>Name</ids:simpleValue>
