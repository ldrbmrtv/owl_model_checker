                        <xs:restriction base="xs:string">
                            <xs:enumeration value="HALF_TURN_RAMP"/>
                            <xs:enumeration value="QUARTER_TURN_RAMP"/>
                            <xs:enumeration value="SPIRAL_RAMP"/>
                            <xs:enumeration value="STRAIGHT_RUN_RAMP"/>
